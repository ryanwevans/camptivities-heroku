<%= form_for counselor do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <br>
  <%= f.label :age %>
  <%= f.text_field :age %>
  <br>
  <br>
  <%= f.label(:admin, "Camp Admin") %><br>
  <%= f.radio_button(:admin, true) %>Yes
  <%= f.radio_button(:admin, false) %>No
  <br><br>
  <%= f.submit %>
<% end %>
<br>
<%= form_for :camp_counselor, url: camp_counselor_path, method: :delete, data: {confirm: "Are You Sure You Want to Delete Your Account?"} do |f| %>
<%= f.submit "Delete Account" %>
<% end %>
